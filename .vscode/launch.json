    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Build LTS Ubuntu 16.04",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/build.ps1",
            "cwd": "${workspaceFolder}",
            "args": [
                "-build",
                "-Channel",
                "lts",
                "-name",
                "ubuntu16.04"
            ]
        }
    ]
}
